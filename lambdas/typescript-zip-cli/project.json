{
    "name": "typescript-zip",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "lambdas/typescript-zip-cli/src",
    "projectType": "application",
    "tags": [],
    "// targets": "to see all targets run: nx show project typescript-zip --web",
    "targets": {
        "deploy": {
            "executor": "nx:run-commands",
            "options": {
                "cwd": "{projectRoot}",
                "command": "make deploy"
            }
        },
        "build": {
            "executor": "nx:run-commands",
            "dependsOn": ["pre-build", "test", "lint"],
            "options": {
                "cwd": "{projectRoot}",
                "command": "make build"
            }
        },
        "pre-build": {
            "executor": "nx:run-commands",
            "options": {
                "cwd": "{projectRoot}",
                "command": "make pre-build"
            }
        }
    }
}
